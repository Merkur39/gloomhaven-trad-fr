{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "lib": ["dom", "esnext"],
    "noEmit": true,
    "jsx": "react",
    "strict": true,
    "pretty": true,
    "sourceMap": true,
    /* Module Resolution Options */
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
  },
  "exclude": [
    "app/test",
    "release",
    "app/main.prod.js",
    "app/renderer.prod.js",
    "app/dist",
    "dll"
  ]
}
